<!doctype HTML>
<html>
  <head>

    <% if (htmlWebpackPlugin.options.devOptions) { %>
    
    <meta http-equiv="Content-type" content="text/html; charset=utf-8"/>
    <title>Script App</title>
    <link href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
    <style>
      .row {
      margin: 10px;
      }
    </style>

    <% } %>

    <title>Member Search</title>
    <link rel="stylesheet" type="text/css" href="https://cdn.jsdelivr.net/npm/animate.css@3.5.1">
    
  </head>
  <body>
    <%
      if (htmlWebpackPlugin.options.devOptions) {
      // anything in this block is only rendered if the Webpack options
      // define a 'devOptions' attribute. This allows us to mock portal
      // functions or content here in this ejs file.
      %>
    <div class="container">
    <span id="theme-userid"><%= htmlWebpackPlugin.options.devOptions.user %></span>
    <% } %>
    <div id="{{portletId}}"></div>
    <%
      if (htmlWebpackPlugin.options.devOptions) {
      %>
    </div>
    <% } %>
  </body>
</html>
